<div class="row">
    <div class="col-sm-12">
        <div class="card-box table-responsive">
            <div class="dropdown pull-right">
                <!-- <a href="#" class="dropdown-toggle card-drop" data-toggle="dropdown" aria-expanded="false">
                    <i class="zmdi zmdi-more-vert"></i>
                </a>
                <ul class="dropdown-menu" role="menu">
                    <li><a href="#">Action</a></li>
                    <li><a href="#">Another action</a></li>
                    <li><a href="#">Something else here</a></li>
                    <li class="divider"></li>
                    <li><a href="#">Separated link</a></li>
                </ul> -->
            </div>
            <h4 class="header-title m-t-0 m-b-30">
                <span style="margin-right: 20px">Category</span>
                <!-- <button type="button" onclick="javascript:window.location='/create'" class="btn btn-primary waves-effect w-md waves-light m-b-5">Create</button> -->
                <a href="category/create" class="btn btn-primary waves-effect w-md waves-light m-b-5">Create</a>               
            </h4>            
            <table id="datatable" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>Name</th>
                        <th>Link</th>
                        <th>Created_at</th>
                        <th>Action</th>
                        
                    </tr>
                </thead>
                <tbody>
                    <% for(var i = 0; i < data.length; i++){ %> 
                        <tr>
                            <td><%= i + 1 %></td>
                            <td><%= data[i].name %></td>
                            <td><a href="/project/<%= data[i]._id %>" target="_blank">/project/<%= data[i]._id %></a></td>
                            <td><%= data[i].created_at %></td>
                            <td>
                                <div class="btn-group btn-group-sm">
                                    <a class="btn btn-warning" style="margin-right: 5px;" href="/admin/category/edit/<%= data[i]._id %>">
                                        <i class="fa fa-edit"> Edit</i>
                                    </a>
                                    <a class="btn btn-danger" href="/admin/category/delete/<%= data[i]._id %>">
                                        <i class="fa fa-trash-o"> Delete</i>
                                    </a>
                                </div>
                            </td>
                        </tr>
                    <% } %>

                </tbody>
            </table>
        </div>
    </div><!-- end col -->
</div>

 
<!-- end row -->