<div class="row">
    <div class="col-sm-12">
        <div class="btn-group pull-right m-t-15">
            <button type="button" class="btn btn-custom dropdown-toggle waves-effect waves-light" data-toggle="dropdown" aria-expanded="false">Settings <span class="m-l-5"><i class="fa fa-cog"></i></span></button>
            <ul class="dropdown-menu" role="menu">
                <li><a href="#">Action</a></li>
                <li><a href="#">Another action</a></li>
                <li><a href="#">Something else here</a></li>
                <li class="divider"></li>
                <li><a href="#">Separated link</a></li>
            </ul>
        </div>
        <h4 class="page-title">Dashboard</h4>
    </div>
</div>
<div class="row">    
    <div class="col-lg-12">
        <div class="card-box">
            <h4 class="header-title m-t-0 m-b-30">Latest Projects</h4>
            <div class="table-responsive">
                <table class="table">
                    <thead>
                    <tr>
                        <th>STT</th>
                        <th>Project Name</th>
                        <th>Link</th>
                        <th>Created Date</th>
                        <th>Action</th>
                    </tr>
                    </thead>
                    <tbody>
                    <% for(var i = 0; i < data.length; i++){ %>
                        <tr>
                            <td><%= i + 1 %></td>
                            <td><%= data[i].name %></td>
                            <td><a href="/project/<%= data[i]._id  %>" target="_blank">/project/<%= data[i]._id  %></a></td>
                            <td><%= data[i].created_at %></td>                            
                            <td>
                                <div class="btn-group btn-group-sm">
                                    <a class="btn btn-warning" style="margin-right: 5px;" href="/admin/category/edit/<%= data[i]._id %>">
                                        <i class="fa fa-edit"> Edit</i>
                                    </a>
                                    <a class="btn btn-danger" href="/admin/category/delete/<%= data[i]._id %>">
                                        <i class="fa fa-trash-o"> Delete</i>
                                    </a>
                                </div>
                            </td>
                        </tr>
                    <% } %>                 
                    </tbody>
                </table>
            </div>
        </div>
    </div><!-- end col -->
</div>