<div class="row">
    <div class="col-sm-12">
        <div class="card-box table-responsive">
            
            <h4 class="header-title m-t-0 m-b-30">
                <span style="margin-right: 20px">Danh sách khách hàng</span>
                <a href="member/create" class="btn btn-primary waves-effect w-md waves-light m-b-5">Thêm mới</a>
            </h4>
            <table id="datatable" class="table table-striped table-bordered">
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Dự án</th>
                        <th>Action</th>                        
                    </tr>
                </thead>               
                <tbody>
                    <% for(var i = 0; i < members.length; i ++){ %>
                        <tr>
                            <td><%= i + 1 %></td>
                            <td><%= members[i].name %></td>
                            <td><%= members[i].email %></td>
                            <td><%=  %></td>
                            <td>
                                <a href="member/edit/<%= members[i]._id %>" style="margin-right: 5px;" class="btn btn-warning">
                                    <i class="fa fa-edit"> Edit</i>
                                </a>
                                <a href="member/delete/<%= members[i]._id %>" class="btn btn-danger">
                                    <i class="fa fa-trash-o"> Delete</i>
                                </a>
                            </td>                        
                            </tr>      
                    <% }  %>
                </tbody>
            </table>
        </div>
    </div><!-- end col -->
</div> 
<!-- end row -->